      "top": null,
      "right": null,
      "left": null,
      "bottom": null
    },
    "&-flTest": {
      "width": 100
    },
    "&-flHomeSearchView": {
      "flexProps": {
        "positionType": "RELATIVE"
      },
      "flex": {
        "positionType": 0
      }
    },
