        "flexGrow": 1
      },
      "layoutManager": {
        "spanCount": 2,
        "scrollDirection": "VERTICAL"
      }
    },
    "&-gviHouse": {
      "flexProps": {
        "positionType": "RELATIVE"
      },
      "flex": {
        "positionType": 0
      }
    },
    "&-gvCategories": {
      "height": 40,
      "layoutManager": {
        "spanCount": 4,
        "scrollDirection": "VERTICAL"
      }
    },
    "&-gviCategory": {
